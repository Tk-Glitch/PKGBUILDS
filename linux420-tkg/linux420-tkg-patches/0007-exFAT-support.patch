+			sb->s_id, MAJOR(bd_dev), MINOR(bd_dev), &vaf);
+#else
+	vsnprintf(msg, sizeof(msg), fmt, args);
+	printk("%s[EXFAT] (%s[%d:%d]): %s\n", level,
+			sb->s_id, MAJOR(bd_dev), MINOR(bd_dev), msg);
+#endif
+	va_end(args);
