+	unsigned int flags;
+
+	if (nd && nd->flags & LOOKUP_RCU)
+		return -ECHILD;
+
+	flags = nd ? nd->flags : 0;
+#else
+	unsigned int flags;
+
+	flags = nd ? nd->flags : 0;
+#endif
+
+	if (dentry->d_inode)
+		return 1;
+
+#if 0
+	if (!flags)
