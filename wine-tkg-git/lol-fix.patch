+    output( "\tlea -8(%%ebp), %%esp\n" );
+    output( "\tpop %%edi\n" );
+    output( "\tpop %%esi\n" );
+    output( "\tleave\n" );
+    /* XXX This probably makes the RSB unhappy, but we need to account for the "add esp, 4" in the syscall. */
+    output( "\tjmpl *(%%esp)\n" );
